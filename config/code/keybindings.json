// Place your key bindings in this file to overwrite the defaults
[
	{"key": "alt+enter", "command": "editor.action.quickFix", "when": "editorTextFocus"},
	{"key": "ctrl+b", "command": "editor.action.goToDeclaration", "when": "editorTextFocus"},
	{"key": "ctrl+d", "command": "editor.action.deleteLines", "when": "editorTextFocus"},
	{"key": "ctrl+shift+d", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus"},
	{"key": "ctrl+shift+t", "command": "workbench.action.quickOpen"},
	{"key": "ctrl+shift+r", "command": "workbench.action.gotoSymbol"},
	{"key": "ctrl+shift+w", "command": "workbench.action.closeAllEditors"}
]